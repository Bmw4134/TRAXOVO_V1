# TRAXOVO Enterprise Production Environment
NODE_ENV=production
PORT=5000
SESSION_SECRET=traxovo-enterprise-secure-key

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/traxovo_enterprise

# Authentication
JWT_SECRET=traxovo-jwt-secure-key
BCRYPT_ROUNDS=12

# CORS Configuration
CORS_ORIGIN=*
CORS_CREDENTIALS=true

# API Configuration
API_RATE_LIMIT=1000
API_TIMEOUT=30000

# File Upload Configuration
MAX_FILE_SIZE=52428800
UPLOAD_PATH=./uploads

# Logging
LOG_LEVEL=info
LOG_FORMAT=combined

# KaizenGPT Canvas Integration
CANVAS_COMPONENTS_ENABLED=true
AUTHENTIC_DATA_ONLY=true
SYNTHETIC_DATA_BLOCKED=true

# TRAXOVO Specific
GAUGE_API_ASSETS=717
GPS_FLEET_DRIVERS=92
FLEET_ZONE=580-582
ANNUAL_SAVINGS=104820